/**index.wxss**/
@import "/font/font.wxss";
.top_div{
  width: 100%;
background: #fff;
}
swiper{
  height: 350rpx;
}
.boxs {
  width: 65%;
  align-items: center;
  display: flex;
  justify-content: space-between;
  margin-left: 3%;
  padding: 60rpx 0 18rpx;
}
.boxs>view {
  flex-shrink: 0;
  margin-right: 40rpx;
}
.diyTab {
  flex-grow: 1;
  overflow: hidden;
  border: 2rpx solid #970407;
  border-radius: 40rpx;
  text-align: center;
  color: white;
}

.diyTab text {
  font-size: 26rpx;
  color: #AEAEAE;
}

.diyTab icon {
  color: #AEAEAE;
}

.banner image {
  width: 100%;
  height: 100%;
  display: block;
}

tab-bar {
  position: absolute;
  bottom: 500rpx;
  left: 0;
}

.box {
  width: 100%;
}

.box image {
  width: 100%;
}

.item {
  display: flex;
  justify-content: space-around;
  text-align: center;
  color: #1b1b1b;
  margin: 20rpx 0;
  font-size: 20rpx;
}

.item image {
  width: 80rpx;
  height: 80rpx;
  vertical-align: middle;
}

.Advertisement {
  background: #f4f4f4;
  text-align: center;
  /* padding: 20rpx; */
}
.Advertisement image{
  width: 100%;
}

.activity .title {
  display: flex;
  overflow: hidden;
  padding: 20rpx;
  align-items: center;
  justify-content: space-between;
  background: #fff;
}

.activity .title .line {
  color: #7d7d7d;
  border-left: 8rpx solid #4b6dfe;
  padding-left: 15rpx;
  font-size: 26rpx;
  margin-left: 20rpx;
  padding-top: 5rpx;
  padding-bottom: 5rpx;
  flex-grow: 1;
  overflow: hidden;
}

.activity .title image {
  width: 140rpx;
  height: 34rpx;
  flex-shrink: 0;
}

.more {
  flex-shrink: 0;
  color: #333;
  font-size: 28rpx;
  margin-top: -10rpx;
}

.more .icon-jiantouyou {
  font-size: 18rpx;
  margin-left: 20rpx;
}

.strip {
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

/* 滑动 */

.gundong {
  color: #c99999;
  white-space: nowrap;
}

::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
}

.gundong .before {
  width: 30%;
  display: inline-block;
  font-size: 28rpx;
  text-align: center;
  background: #970407;
  color: white;
  padding: 20rpx 0;
}

.gundong .before .txt {
  font-size: 26rpx;
}

.activity {
  padding-bottom: 20rpx;
  border-bottom: 15rpx solid #f4f4f4;
}

.actGoods {
  width: 96%;
  margin: 0 auto;
  display: flex;
  /* justify-content: space-between; *//* text-align: center; */
  padding-top: 20rpx;
}

.name {
  height: 40rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 28rpx;
  margin: 15rpx 0rpx;
  display: block;
}

.price {
  font-size: 28rpx;
  color: #dd191d;
}

.reality {
  font-size: 32rpx;
  font-weight: bold;
  margin-right: 15rpx;
}

.Original {
  color: #7d7d7d;
  text-decoration: line-through;
  font-size: 28rpx;
}

.gundong .active {
  font-weight: bold;
  background: #000;
}

.jiu .title .line {
  border-left: 8rpx solid #ff353a;
}

.jiu .goodsList text text {
  color: #dd191d;
}

.low {
  display: flex;
  overflow: hidden;
  font-size: 24rpx;
}
.low .imglow{
  text-align: center;
  display: inline-block;
  position: relative;
}

.low .imglow image {
  width: 103rpx;
  height: 37rpx;
  margin-right: 10rpx;
}
.low .imglow text {
  width: 100rpx;
  position: absolute;
  left: 0;
  top: 0;
  text-align: center;
  line-height: 37rpx;
  color: #fff;
font-size: 20rpx;
}

.low text {
  color: #dd191d;
  line-height: 36rpx;
  flex-shrink: 0;
}
.low text.txt{
  line-height: 30rpx;
  margin-top: 6rpx;
}
.low text.num02{
  line-height: 30rpx;
  margin-top: 6rpx;
}

.kanjia .title .line {
  border-left: 8rpx solid #ffc118;
}

.Bargain {
  color: #dd191d;
  font-size: 28rpx;
  margin-top: 10rpx;
  display: flex;
  overflow: hidden;
  align-items: center;
  justify-content: space-between;
}

.Bargain .price {
  flex-grow: 1;
}

.Bargain .price text {
  font-size: 32rpx;
  font-weight: bold;
  margin-left: 0rpx;
}

.Bargain text.btn {
  width: 120rpx;
  line-height: 50rpx;
  background: #dd191d;
  border-radius: 40rpx;
  text-align: center;
  color: white;
  font-size: 24rpx;
  flex-shrink: 0;
}

/* 今日主推 */

.content {
  display: flex;
  font-size: 16px;
  padding: 15rpx 29rpx;
  border-bottom: 1rpx solid #eee;
  align-items: center;
  justify-content: space-between;
}

.content image {
  width: 200rpx;
  margin-right: 25rpx;
  height: 200rpx;
  flex-shrink: 0;
}

.content  text {
  display: inline-block;
}

.content >view {
  flex-grow: 1;
  overflow: hidden;
}

.content view .num {
  color: #7d7d7d;
  font-size: 26rpx;
}

.goodSname {
  color: #1b1b1b;
}

.content .price {
  margin: 20rpx 0;
  font-size: 28rpx;
}

.content .price text:first-child {
  font-size: 32rpx;
  color: #d50000;
  margin-left: -10rpx;
  font-weight: bold;
}

.stock {
  margin: 20rpx 0;
  font-size: 12px;
  color: #434343;
}

.goodSname {
  margin-top: 30rpx;
}

.through {
  text-decoration: line-through;
  color: #e5e5e5;
  margin-left: 20rpx;
}

.goShop {
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  font-size: 24rpx;
  color: white;
  background: #dd191d;
  padding: 10rpx 15rpx;
  border-radius: 15rpx;
}

.content>view {
  position: relative;
}

.num01, .num02 {
  font-weight: bold;
}

.num01 {
  font-size: 32rpx;
}

.item .txt {
  font-size: 28rpx;
  color: #333;
}

.actGoods scroll-view {
  width: 100%;
  white-space: nowrap;
}

.cc view {
  width: 300rpx;
  height: 300rpx;
  float: left;
}

.actGoods .goodsList {
  width: 32.3%;
  margin-right: 1%;
  display: inline-block;
}

.actGoods .goodsList .img {
  width: 100%;
  height: 0;
  padding-bottom: 100%;
  position: relative;
}

.goodsList .img image {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

.goodsList.goodSpike .name {
  -webkit-line-clamp: 1;
  height: 40rpx;
}

.content .goodSname {
  margin-top: 0;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.prop_div{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1000;
}
.activity_empty{
  width: 100%;
  line-height: 200rpx;
  color: #666;
  font-size: 30rpx;
  text-align: center;
}
.prop_div .prop_content{
  /* width: 400rpx; */
  margin: 0 auto;
  padding: 30rpx 20rpx;
  position: relative;
  text-align: center;
  display: inline-block;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
}
.prop_content .coupon_div{
  position: relative;
}
.prop_content .coupon_div image{
  width: 500rpx;
}
.prop_content .coupon_div .coupon{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}
.coupon_div .coupon .title{
  flex-shrink:0;
  margin-top: 100rpx;
  color: #7a0000;
  font-size: 30rpx; 
}
.coupon_div .coupon .money{
  flex-grow: 1;
  overflow: hidden;
  text-align: center;
  font-size: 30rpx;
  color: #E31537;
  padding-top: 16rpx;
  margin-left: -20rpx;
}
.coupon_div .coupon .money .price{
  font-size: 100rpx;
}
.coupon_div .coupon .btn{
  text-align: center;
  margin-bottom: 40rpx;
}
.coupon_div .coupon .btn text{
  width: 200rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #E31537;
  text-align: center;
  background: #facf9c;
  border-radius: 40rpx;
  display: inline-block;
}
.prop_div .prop_content .icon-guanbi{
  margin-top: 40rpx;
  font-size: 60rpx;
  color: #fff;
}