/**app.wxss**/
@import'/font/font.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
/* tab-bar{
  position: absolute;
  bottom: 100rpx;
  left: 0;
} */
.empty_div{
  line-height: 400rpx;
  text-align: center;
  font-size: 30rpx;
  color: #666;
}

.newTab {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 124rpx;
  z-index: 1;
  background: linear-gradient(rgba(255, 255, 255, .05), rgba(255, 255, 255, .9));
}